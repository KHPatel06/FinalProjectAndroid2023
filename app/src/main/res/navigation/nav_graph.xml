<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/shopFragment"
        android:name="com.example.finalprojectandroid2023.ShopFragment"
        android:label="fragment_shop"
        tools:layout="@layout/fragment_shop" >
        <action
            android:id="@+id/action_shopFragment_to_sellFragment"
            app:destination="@id/sellFragment" />
    </fragment>
    <fragment
        android:id="@+id/sellFragment"
        android:name="com.example.finalprojectandroid2023.SellFragment"
        android:label="SellFragment" >
        <action
            android:id="@+id/action_sellFragment_self"
            app:destination="@id/sellFragment" />
    </fragment>
    <fragment
        android:id="@+id/mainFragment"
        android:name="com.example.finalprojectandroid2023.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" >
        <action
            android:id="@+id/action_mainFragment_to_shopFragment"
            app:destination="@id/shopFragment" />
    </fragment>
</navigation>